{
  "name": "terminal7",
  "version": "0.3.0",
  "description": "The terminal incarnated as a fingered tmux client",
  "main": "src/index.js",
  "private": true,
  "bin": {
    "terminal7": "download_browser.js"
  },
  "build": {
    "appId": "com.tuzig.terminal7",
    "files": [
      "index.html",
      "css/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-webpack": "^1.0.5",
    "hammerjs": "^2.0.8",
    "web": "0.0.2",
    "websocket-extensions": ">=0.1.4",
    "xterm": "^4.5.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^3.5.2",
    "karma": "^5.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "<8",
    "puppeteer": "^3.1.0",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "karma start --single-run karma.conf.js",
    "build": "npx webpack --output dist/terminal7.js",
    "ios": "cordova run ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afittestide/terminal7.git"
  },
  "keywords": [
    "terminal",
    "javascript",
    "tmux",
    "webrtc"
  ],
  "author": "Benny Daon",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/afittestide/terminal7/issues"
  },
  "homepage": "https://github.com/afittestide/terminal7#readme",
  "mocha": {
    "watch-files": [
      "src/*.js",
      "test/*.js"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-webpack": {}
    },
    "platforms": [
      "ios",
      "browser"
    ]
  }
}
