{
  "name": "terminal7",
  "version": "0.9.1",
  "description": "A touchable terminal emulator & multiplexer tablet app",
  "main": "src/index.js",
  "private": true,
  "bin": {
    "terminal7": "download_browser.js"
  },
  "build": {
    "appId": "dev.terminal7",
    "files": [
      "index.html",
      "css/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "codemirror": "git+https://github.com/tuzig/CodeMirror.git",
    "cordova": "^10.0.0",
    "cordova-plugin-inappbrowser": "^4.0.0",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-plugin-ssh-connect": "^1.1.1",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-webpack": "^1.0.5",
    "cordova-plugin-whitelist": "1",
    "glob": "^7.1.6",
    "hammerjs": "^2.0.8",
    "karma-safari-launcher": "^1.0.0",
    "karma-source-map-support": "^1.4.0",
    "npm": "^6.14.8",
    "uuid": "^8.3.1",
    "web": "0.0.2",
    "websocket-extensions": ">=0.1.4",
    "xterm": "^4.5.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-search": "^0.7.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cordova-browser": "^6.0.0",
    "cordova-clipboard": "^1.3.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "css-loader": "^3.5.2",
    "file-loader": "^6.1.0",
    "karma": "^5.2.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "<8",
    "puppeteer": "^3.1.0",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.1.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "karma start --single-run",
    "build": "npx webpack --output dist/terminal7.js",
    "ios": "cordova run ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuzig/terminal7.git"
  },
  "keywords": [
    "terminal",
    "javascript",
    "tmux",
    "webrtc"
  ],
  "author": "Benny Daon",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/tuzig/terminal7/issues"
  },
  "homepage": "https://github.com/tuzig/terminal7#readme",
  "mocha": {
    "watch-files": [
      "src/*.js",
      "test/*.js"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-webpack": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-ssh-connect": {},
      "cordova-clipboard": {}
    },
    "platforms": [
      "ios",
      "browser"
    ]
  }
}
