<?xml version='1.0' encoding='utf-8'?>
<widget id="dev.afittestide.terminal7" version="0.3.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Terminal7</name>
    <description>
        A gestures aware terminal emulator and multiplexer
    </description>
    <author email="dev@tuzig.com" href="http://afittestide.dev/terminal7">
        Tuzig LTD
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
